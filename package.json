{
  "name": "uranus",
  "version": "1.5.0",
  "description": "Yet another validation library for node that validates up to ur-anus.",
  "main": "./dist/index.js",
  "scripts": {
    "eslint": "./node_modules/.bin/eslint --config ./.eslintrc ./src/. || true",
    "clean": "rm -rf ./dist/",
    "build": "npm run clean && ./node_modules/.bin/babel --modules umd --optional runtime --source-maps inline --no-non-standard --out-dir ./dist/. ./src/. || true",
    "prepublish": "npm run build",
    "pretest": "npm run eslint && npm run build",
    "test": "./node_modules/.bin/mocha --compilers js:babel/register --reporter spec tests/.",
    "debug": "./node_modules/.bin/mocha --compilers js:babel/register --reporter spec --debug-brk --inline-diffs --watch tests/."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/umayr/uranus.git"
  },
  "author": "Umayr Shahid <umayrr@hotmail.co.uk>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/umayr/uranus/issues"
  },
  "homepage": "https://github.com/umayr/uranus#readme",
  "devDependencies": {
    "babel": "^5.8.23",
    "eslint": "^1.3.1",
    "mocha": "^2.2.5"
  },
  "keywords": [
    "validate",
    "validation",
    "input",
    "validation",
    "api",
    "assert",
    "library"
  ],
  "dependencies": {
    "babel-runtime": "^5.8.20",
    "cressida": "^0.4.0",
    "validator": "^4.0.0"
  }
}
